{
  "name": "quizeek",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "ui:add": "npx shadcn@latest add --src-dir --path src/components/ui",
    "db:start:dev": "dotenv -e .env.development -- turso dev --db-file quizeek-dev.db",
    "db:push:dev": "dotenv -e .env.development -- npx drizzle-kit push",
    "db:studio:dev": "dotenv -e .env.development -- npx drizzle-kit studio",
    "db:start:prod": "dotenv -e .env.production -- turso dev --db-file quizeek-dev.db",
    "db:push:prod": "dotenv -e .env.production -- npx drizzle-kit push",
    "db:studio:prod": "dotenv -e .env.production -- npx drizzle-kit studio"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.37.0",
    "lucide-react": "^0.462.0",
    "next": "15.0.3",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "dotenv-cli": "^7.4.4",
    "drizzle-kit": "^0.29.0",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-prefer-arrow-functions": "^3.4.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-unused-imports": "^4.1.4",
    "postcss": "^8",
    "prettier": "^3.4.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
